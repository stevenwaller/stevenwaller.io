{
  "name": "hugo-starter",
  "version": "1.0.0",
  "description": "This is a boilerplate for using [Hugo](https://gohugo.io/) as a static site generator and Gulp + Webpack as your asset pipeline",
  "main": "index.js",
  "scripts": {
    "old-hugo": "gulp hugo",
    "old- build": "node check-for-outdated-dependencies.js && rm -rf ./dist && cross-env NODE_ENV=production gulp build",
    "old-build-preview": "node check-for-outdated-dependencies.js && rm -rf ./dist && cross-env NODE_ENV=production gulp build-preview",
    "old-dev": "node check-for-outdated-dependencies.js && rm -rf ./dist && gulp dev",
    "old-lint": "eslint src",
    "build": "npm run clean && npm-run-all build:assets build:html build:sass build:autoprefixer \"build:js --mode production\" asset-cache-bust",
    "dev": "npm run clean && npm-run-all --parallel watch serve",
    "clean": "rimraf dist && mkdir dist",
    "watch": "npm-run-all --parallel \"build:* --watch\"",
    "serve": "browser-sync start --server dist --files dist --no-ghost-mode --no-open",
    "build:assets": "cpx \"src/**/*.!(html|md|scss|js|svg)\" dist",
    "build:html": "./bin/hugo.darwin --config config.toml -v",
    "build:sass": "node-sass-chokidar src/styles/ --source-map dist/css --output .temp --output-style compressed",
    "build:autoprefixer": "postcss \".temp/*.css\" --use autoprefixer --dir dist/css",
    "build:lint": "esw \"src/scripts/{,**/}*.js\"",
    "build:js": "webpack"
  },
  "author": "",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "lodash": "4.17.10",
    "modernizr": "3.6.0"
  },
  "devDependencies": {
    "autoprefixer": "9.0.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "2.24.6",
    "cpx": "^1.5.0",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "eslint": "5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-watch": "^4.0.2",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "gulp": "^3.9.1",
    "gulp-babel": "7.0.1",
    "gulp-cache-bust": "1.3.0",
    "gulp-notify": "3.2.0",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-util": "^3.0.7",
    "imports-loader": "0.8.0",
    "json-loader": "^0.5.7",
    "modernizr-loader": "^1.0.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "prettier": "^1.14.0",
    "rimraf": "^2.6.2",
    "run-sequence": "2.2.1",
    "url-loader": "1.0.1",
    "webpack": "4.16.3",
    "webpack-cli": "^3.1.0"
  }
}
