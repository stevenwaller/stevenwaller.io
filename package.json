{
  "name": "hugo-starter",
  "version": "1.0.0",
  "description": "This is a boilerplate for using [Hugo](https://gohugo.io/) as a static site generator and Gulp + Webpack as your asset pipeline",
  "main": "index.js",
  "scripts": {
    "hugo": "gulp hugo",
    "build": "node check-for-outdated-dependencies.js && rm -rf ./dist && cross-env NODE_ENV=production gulp build",
    "build-preview": "node check-for-outdated-dependencies.js && rm -rf ./dist && cross-env NODE_ENV=production gulp build-preview",
    "dev": "node check-for-outdated-dependencies.js && rm -rf ./dist && gulp dev",
    "lint": "eslint src"
  },
  "author": "",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "jquery": "3.3.1",
    "jquery.easing": "^1.4.1",
    "lodash": "4.17.5",
    "modernizr": "3.6.0",
    "postal": "^2.0.5"
  },
  "devDependencies": {
    "autoprefixer": "8.1.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "6.26.0",
    "browser-sync": "2.23.6",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "eslint": "4.19.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "exports-loader": "0.7.0",
    "file-loader": "1.1.11",
    "gulp": "^3.9.1",
    "gulp-babel": "7.0.1",
    "gulp-cache-bust": "1.3.0",
    "gulp-notify": "3.2.0",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "2.6.4",
    "gulp-util": "^3.0.7",
    "imports-loader": "0.8.0",
    "modernizr-loader": "^1.0.1",
    "run-sequence": "2.2.1",
    "url-loader": "1.0.1",
    "webpack": "4.1.1",
    "yarn": "1.5.1"
  }
}
